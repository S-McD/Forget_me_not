<!-- landing page for all of a users created wishlists

It shows a list of all wishlists
Button to create a new wishlist -->
 <!DOCTYPE html>
 <html lang="en">
 <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Wishlists</title>
 </head>
 <body>
    <%- include('partials/signed_in_header') %>

      <% wishlists.forEach( (wishlist) => { %>
         <h1> <%=wishlist.name %> </h1>

         <form method="get" action="/wishlist/wishlist_invite/<%=wishlist._id%>">
            <button type="submit"> Invite </button>
         </form>
         <form method="post" action="/wishlist/assignEvent/<%=wishlist._id%>">
            <input type="text" name="eventName">
            <button type="submit">Assign to an Event</button>
         </form>
      <% }) %>

    <form method = 'GET' action = 'wishlist/new_wishlist'> 
      <input type = 'submit' value = 'Create Wishlist'>
    </form> 

    <%- include('partials/footer') %>
 </body>
 </html>
